<%


var log = new Log();
if(log.isDebugEnabled()){
    log.debug("INSIDE get process details...");
}

var processPath = String(request.getParameter("process_path"));


log.info("fetching the process details from the registry...");
var ps = new Packages.org.wso2.carbon.processCenter.core.ProcessStore;

var processDetails = ps.getProcessDetails(processPath);
log.info(processDetails);
if(log.isDebugEnabled()){
    log.debug(processDetails);
}
print(processDetails);
%>