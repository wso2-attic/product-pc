<div id="overviewDiv">
        <div class="control-group">
            <h2 class="field-title">
                <a class="collapsing-h2" aria-expanded="true" href="#collapseprocessinfo" data-toggle="collapse">
                    <i class="cu-btn-exp-col btn-collapsed">Process Information</i>
                </a>
            </h2>
            <div class="collapse in responsive-form-container" id="collapseprocessinfo">
                <form class="form-horizontal">
                    <div class="form-group">
                        <label class="custom-form-label col-lg-2 col-md-2 col-sm-12 col-xs-12">Process name<sup class="required-field">*</sup></label>
                        <div class="custom-form-right col-lg-5 col-md-8 col-sm-8 col-xs-12">
                            <input onblur="blurIt()" type="text" placeholder="Enter Process Name" name="overview_name" id="pName" class="form-control" required="">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="custom-form-label col-lg-2 col-md-2 col-sm-12 col-xs-12">Version<sup class="required-field">*</sup></label>
                        <div class="custom-form-right col-lg-5 col-md-8 col-sm-8 col-xs-12">
                            <input onblur="blurIt()" type="text" placeholder="Enter Process Version" name="overview_version" id="pVersion" class="form-control" required="">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="custom-form-label col-lg-2 col-md-2 col-sm-12 col-xs-12">Owner<sup class="required-field">*</sup></label>
                        <div class="custom-form-right col-lg-5 col-md-8 col-sm-8 col-xs-12">
                            <input onblur="blurIt()" type="text" placeholder="Enter Process Owner" name="overview_owner" id="pOwner" class="form-control" required="">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="custom-form-label col-lg-2 col-md-2 col-sm-12 col-xs-12">Tags</label>
                        <div class="custom-form-right col-lg-5 col-md-8 col-sm-8 col-xs-12">
                            <select id="_tags" class="tag-ui-selectbox form-control select2-hidden-accessible" multiple="" name="_tags" tabindex="-1" aria-hidden="true">
                            </select>
                            <span id = "span" class="select2 select2-container select2-container--default select2-container--below" dir="ltr" style="width: 142px;">
                                <span class="selection">
                                    <span class="select2-selection select2-selection--multiple" role="combobox" aria-autocomplete="list" aria-haspopup="true" aria-expanded="false" tabindex="0">
                                        <ul class="select2-selection__rendered">
                                            <li id="tag-box-list" class="select2-search select2-search--inline">
                                                <input id="tag-box" class="select2-search__field" type="search" tabindex="-1" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" role="textbox" placeholder="" style="width: 2.25em;">
                                            </li>
                                        </ul>
                                        <span class="select2-container--default select2-container--open select2-container--focus">
                                            <span class="select2-dropdown select2-dropdown--above" dir="ltr" style="width: 142px">
                                                <span class="select2-results">
                                                    <ul class="select2-results__options" role="tree" aria-multiselectable="true" id="select2-_tags-6j-results" aria-expanded="true" aria-hidden="false">
                                                        <li id="select2-results__option" role="treeitem" class="select2-results__option" style="display: none">Please enter 2 or more characters</li>
                                                        <li id="select2-results__option--highlighted" class="select2-results__option select2-results__option--highlighted" role="treeitem" aria-selected="false" style="display: none" onclick="addNewTag()"></li>
                                                    </ul>
                                                </span>
                                            </span>
                                        </span>
                                    </span>
                                </span>
                                <span class="dropdown-wrapper" aria-hidden="true"></span>
                            </span>
                        </div>
                    </div>
                </form>

                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-10">
                        <button class="btn btn-primary" onclick="showTextEditor(this)">Describe process in text</button>
                        <button class="btn btn-primary" onclick="associateBPMN(this)">Associate BPMN model</button>
                        <button class="btn btn-primary" onclick="designBPMN(this)">Design BPMN</button>
                        <button class="btn btn-primary" onclick="associateFlowChart(this)">Associate Flow Chart</button>
                        <button class="btn btn-primary" onclick="associateDocument(this)">Documents</button>
                        <button class="btn btn-primary" onclick="associatePdf(this)">Associate PDF</button>


                    </div>
                </div>

            </div>

            <h2 class="field-title">
