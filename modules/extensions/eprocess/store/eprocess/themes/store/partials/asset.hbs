<script>
    var isSocial = ('{{features.social.enabled}}' == 'true');

</script>
<div class="asset-description">
    {{#with assets}}
        {{#with attributes}}
            <div class="asset-description-header">
                <div class="row">
                    <div class="span9" style="height:auto">
                        <div>
                            <div id="processName">
                                <h3>{{overview_name}}</h3>
                            </div>
                            <small> {{t "Version"}} {{overview_version}}</small>
                            <small> {{t "by"}} {{overview_provider}} </small>
                            <span class="asset-rating">
                            <div style="width:{{../ratingPx}}px"></div>
                            </span>
                            <hr width="800">
                            <div id="buttons">
                                <a href='{{url ""}}/asts/process/variant?id={{id}}'
                                   class="btn btn-primary"
                                   style="margin-right:10px;">Authorizations</a>
                                <a href='{{url ""}}/asts/process/variant?id={{id}}'
                                   class="btn btn-primary" style="margin-right:10px;">Validate</a>
                                <a href='{{url ""}}/asts/process/variant?id={{id}}'
                                   class="btn btn-primary" style="margin-right:10px;">Runtime
                                                                                      Analysis</a>
                                <a href='{{url ""}}/asts/process/variant?id={{id}}'
                                   class="btn btn-primary" style="margin-right:10px;">Report an
                                                                                      Issue</a>
                                <a href='{{url ""}}/asts/process/variant?id={{id}}'
                                   class="btn btn-primary" style="margin-right:10px;">Deploy</a>
                                <a href='{{url ""}}/asts/process/variant?id={{id}}'
                                   class="btn btn-primary" style="margin-right:10px;">Start</a>

                            </div>
                            <hr width="800">
                            <div id="processArchive">
                                <table>
                                    <tbody>
                                    <tr>
                                        <td style="width: 200px;padding-top: 2px;"><b><label
                                                class="control-label">Process Archive</label></b>
                                        </td>
                                        <td style="padding-right: 15px;"><label
                                                id="lblProcessArchive"
                                                class="control-label">{{images_processArchive}}</label>
                                        </td>
                                        <td>
                                            <a href='{{url ""}}/storage/eprocess/c4209699-1884-4c54-a85f-558a5abcfaca/{{images_processArchive}}'
                                               class="btn btn-primary"
                                               style="float:right;">Download</a></td>
                                    </tr>
                                    </tr>

                                    </tbody>
                                </table>
                            </div>
                            <hr width="800">

                            <div id="ast-description"
                                 style="margin-left : 10px; margin-right :10px; padding-top:30px;text-align: justify">{{overview_description}}</div>

                            <hr width="800">

                            <div id="proptable">
                                <table id="htmlgrid" class="testgrid">
                                    <tr>
                                        <th>Properties</th>
                                        <th>Value</th>
                                    </tr>
                                    <tr id="R1">
                                        <td>Owner</td>
                                        <td>{{properties_owner}}</td>
                                    </tr>
                                    <tr id="R2">
                                        <td>Deployment URL</td>
                                        <td>{{properties_deploymentUrl}}</td>
                                    </tr>
                                    <tr id="R3">
                                        <td>Web App URL</td>
                                        <td>{{properties_webappUrl}}</td>
                                    </tr>
                                    <tr id="R4">
                                        <td>IT Support</td>
                                        <td>{{properties_itSupport}}</td>
                                    </tr>
                                    <tr id="R5">
                                        <td>Developers</td>
                                        <td>{{properties_developers}}</td>
                                    </tr>
                                    <tr id="R6">
                                        <td>Predecessors</td>
                                        <td id="td_pre">{{properties_predecessors}}</td>
                                    </tr>
                                    <tr id="R7">
                                        <td>Successors</td>
                                        <td id="td_suc">{{properties_sucessors}}</td>
                                    </tr>
                                    <tr id="R8">
                                        <td>Generalizations</td>
                                        <td id="td_gen">{{properties_generalizations}}</td>
                                    </tr>
                                    <tr id="R9">
                                        <td>Specialization</td>
                                        <td id="td_spec">{{properties_specializations}}</td>
                                    </tr>
                                </table>
                            </div>

                        </div>

                    </div>

                </div>

            </div>
            <div class="assetp-properties">

                <ul class="nav nav-tabs" id="assetp-tabs" data-aid="{{../path}}">
                    <li class="active">
                        <a href="#tab-reviews" data-toggle="tab"
                           data-type="comments">{{t "User Reviews"}}</a>
                    </li>
                </ul>

                <div class="tab-content">

                    <div class="tab-pane active" id="tab-reviews">
                        {{#if ../../features.social.enabled}}
                            <iframe name="socialIfr" id="socialIfr"
                                    src="{{../../../features.social.keys.socialAppUrl}}?target={{../../../rxt.shortName}}:{{../../id}}&user={{../../../cuser.username}}"
                                    width="100%" height="100%" style="height: 300px;"
                                    data-script="{{../../../features.social.keys.socialScriptSource}}"
                                    data-script-type="{{../../../features.social.keys.socialScriptType}}"></iframe>
                        {{/if}}
                        {{#if ../../inDashboard}}
                            <div id="comment-content" class="content"></div>
                        {{else}}
                            <div id="comment-content" class="content user-review"></div>
                        {{/if}}
                    </div>

                </div>

            </div>
        {{/with}}
    {{/with}}
</div>
